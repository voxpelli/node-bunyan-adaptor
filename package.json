{
  "name": "bunyan-adaptor",
  "version": "5.0.1",
  "description": "Maps the major Bunyan logging methods to custom methods",
  "homepage": "http://github.com/voxpelli/node-bunyan-adaptor",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-bunyan-adaptor"
  },
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "0BSD",
  "keywords": [
    "bunyan",
    "logging",
    "pino",
    "adapter",
    "console.log",
    "mocking"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "index.d.ts.map"
  ],
  "scripts": {
    "build:0": "run-s clean",
    "build:1:declaration": "tsc -p declaration.tsconfig.json",
    "build:1:commonjs": "linemod -e mjs index.js",
    "build:1": "run-p build:1:*",
    "build": "run-s build:*",
    "check:0": "run-s clean",
    "check:1:dependency-check": "dependency-check *.js 'test/**/*.js' --no-dev -i type-fest",
    "check:1:installed-check": "installed-check -i eslint",
    "check:1:lint": "eslint .",
    "check:1:skypack": "package-check",
    "check:1:tsc": "tsc",
    "check:1": "run-p -c --aggregate-output check:1:*",
    "check:2": "run-s build:1:declaration",
    "check:3": "type-coverage --detail --strict --at-least 99 --ignore-files 'test/*'",
    "check": "run-s check:*",
    "clean:declarations": "rm -rf $(find . -maxdepth 2 -type f -name '*.d.ts')",
    "clean": "run-p clean:*",
    "prepublishOnly": "run-s build",
    "sync-gh-actions": "ghat",
    "test:mocha": "c8 --reporter=lcov --reporter text mocha 'test/**/*.spec.js'",
    "test-ci": "run-s test:*",
    "test": "run-s check test:*"
  },
  "husky": {
    "hooks": {
      "pre-push": "run-s test"
    }
  },
  "devDependencies": {
    "@scoop/bunyan": "2.0.2",
    "@skypack/package-check": "^0.2.2",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "^12.20.12",
    "@types/pino": "^6.3.8",
    "@types/sinon": "^9.0.11",
    "@types/sinon-chai": "^3.2.5",
    "@voxpelli/eslint-config": "^11.0.1",
    "@voxpelli/eslint-config-jsdoc-ts": "^0.3.1",
    "@voxpelli/tsconfig": "^2.0.1",
    "c8": "^7.6.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "dependency-check": "^4.1.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.13",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "ghat": "^0.14.0",
    "husky": "^4.0.0",
    "installed-check": "^4.0.0",
    "linemod": "^0.1.1",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "pino": "^6.11.2",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "type-coverage": "^2.17.5",
    "typescript": "^4.2.4"
  }
}
