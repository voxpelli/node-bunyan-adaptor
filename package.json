{
  "name": "bunyan-adaptor",
  "version": "4.0.1",
  "description": "Maps the major Bunyan logging methods to custom methods",
  "homepage": "http://github.com/voxpelli/node-bunyan-adaptor",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-bunyan-adaptor"
  },
  "author": {
    "name": "Pelle Wessman",
    "email": "pelle@kodfabrik.se",
    "url": "http://kodfabrik.se/"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "check:dependency-check": "dependency-check *.js 'test/**/*.js' --no-dev -i type-fest",
    "check:installed-check": "installed-check -i eslint",
    "check:eslint": "eslint .",
    "check:tsc": "tsc",
    "check": "run-p check:*",
    "declaration:build": "rm -f index.d.ts && tsc -p declaration.tsconfig.json",
    "declaration:check": "git diff --exit-code -- index.d.ts",
    "full-test": "run-s declaration:* test",
    "prepublishOnly": "run-s full-test",
    "test:mocha": "cross-env NODE_ENV=test c8 --reporter=lcov --reporter text mocha test/*.spec.js",
    "test": "run-s check test:*"
  },
  "husky": {
    "hooks": {
      "pre-push": "run-s full-test"
    }
  },
  "devDependencies": {
    "@scoop/bunyan": "2.0.2",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.2.0",
    "@types/node": "^12.19.15",
    "@types/pino": "^6.0.1",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@voxpelli/eslint-config": "^10.0.0",
    "@voxpelli/eslint-config-jsdoc-ts": "^0.3.1",
    "c8": "^7.4.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "dependency-check": "^4.1.0",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^24.0.0",
    "husky": "^4.0.0",
    "installed-check": "^4.0.0-0",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "pino": "^6.3.0",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.5.0",
    "typescript": "^4.1.3"
  }
}
